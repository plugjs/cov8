{
  "name": "@plugjs/cov8",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./coverage": {
      "require": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.cjs"
      },
      "import": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.mjs"
      }
    }
  },
  "scripts": {
    "build": "plug"
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@plugjs/eslint": "^0.1.0",
    "@plugjs/eslint-plugin": "^0.1.0",
    "@plugjs/plug": "^0.1.0",
    "@plugjs/typescript": "^0.1.0",
    "@types/node": "<17",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/parser": "^7.20.2",
    "@babel/types": "^7.20.2",
    "@plugjs/cov8-html": "^0.1.0",
    "source-map": "^0.7.4"
  },
  "peerDependencies": {
    "@plugjs/plug": "^0.1.0"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
